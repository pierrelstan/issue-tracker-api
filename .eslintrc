 {
    "parser": "@typescript-eslint/parser",
    "extends": [],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
    },
    "env": {
      "es6": true,
      "node": true,
    },
    "rules": {
      "no-var": "warn",
      "semi": "warn",
      "indent": ["warn", 2, { "SwitchCase": 1 }],
      "no-multi-spaces": "warn",
      "space-in-parens": "warn",
      "no-multiple-empty-lines": "warn",
      "prefer-const": "warn",
    }
  }
